{"$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "title": {
    "text": "England map",
    "subtitle":["Local Authorities, Mercator Projection",""],
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "middle",
    "color": "black"},

  "config": {
      "background":"white",
      "view": {"stroke": "transparent"}},
  
  "width": 500,
  "height": 600,
  
  "data": {
    "url": "https://raw.githubusercontent.com/jesme00/jesme00.github.io/main/UK2013LAD.json",
"format":{
      "property":"features"}},


  "projection": {"type": "mercator"},
  
  "mark": {
      "type": "geoshape",
      "fill": "lightgray",
      "stroke": "white",
      "strokeWidth":0.1}
}
