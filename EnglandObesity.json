{"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
 
  "title": {
    "text": "Rate of Adults Classified as Overweight or Obese",
    "subtitle":["By Local Authority area. Source: ONS and PHE"],
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "middle",
    "color": "black"},
 
  "config": {
      "background":"white",
      "view": {"stroke": "transparent"}},
 
  "width": 550,
  "height": 700,
 
  "data": {
    "url": "https://raw.githubusercontent.com/jesme00/jesme00.github.io/main/UKLAD.json",
"format":{
      "property":"features"}},
 
"transform": [
        {
          "lookup":"properties.LAD13NM",
          "from":{
              "key": "Area Name",
              "fields": ["Value"],
              "data":{"url":"https://raw.githubusercontent.com/jesme00/jesme00.github.io/main/Percentage%20of%20adults%20classified%20as%20overweight%20or%20obese.csv"}}
              },
              {"calculate":"parseInt(datum.Value)", "as": "% of local population"}
     ],
 
  "projection": {"type": "mercator"},
 
  "mark": {
      "type": "geoshape",
      "stroke": "white",
      "strokeWidth":0.1},
 
  "encoding":{
      "tooltip":[{
      "field":"properties.LAD13NM","type":"nominal","title":"Local Authority"},{"field":"% of local population","title":"% of local population"}],
      "color":{
      "field":"% of local population",
      "type": "quantitative",
      "scale":{
        "scheme":"lightmulti",
        "domain":[43, 77]}
        }

    }
}
